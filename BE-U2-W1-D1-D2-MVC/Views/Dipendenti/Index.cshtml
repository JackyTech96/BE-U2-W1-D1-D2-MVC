
@{
    ViewBag.Title = "Index";
}


<h2 class="my-4">Elenco dipendenti Azienda Edile</h2>


<table class="table table-bordered table-info table-striped">
    <thead>
        <tr>
            <th>Nome Dipendente</th>
            <th>Cognome Dipendente</th>
            <th>Indirizzo</th>
            <th>Codice Fiscale</th>
            <th>Coniugato</th>
            <th>Figli a carico</th>
            <th>Mansione</th>
        </tr>
    </thead>
    <tbody>



        @foreach (BE_U2_W1_D1_D2_MVC.Models.Dipendente d in Model)
        {
            <tr>
                <td>@d.Nome</td>
                <td>@d.Cognome</td>
                <td>@d.Indirizzo</td>
                <td>@d.CF</td>
                <td>@d.Sposato</td>
                <td>@d.Numero_Figli</td>
                <td>@d.Mansione</td>
            </tr>
        }

    </tbody>
</table>
